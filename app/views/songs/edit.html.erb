<h1>Update song form</h1>
<%= form_for(@song) do |f| %>
  <%= f.label 'Song name: ' %>
  <%= f.text_field :name %><br>
  <%= f.label 'Song artist ID: ' %>
  <%= f.text_field :artist_id %><br>
  <%= f.label 'Song genre ID: ' %>
  <%= f.text_field :genre_id %><br>
  <%= f.submit 'Update Song' %>
<% end %>